{
  "name": "lyrifind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "oxlint . --type-aware",
    "lint:fix": "oxlint . --type-aware --fix",
    "format": "prettier --write \"src/**/*.{ts,vue,js,json,css,scss,md}\" \"e2e/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,vue,js,json,css,scss,md}\" \"e2e/**/*.ts\"",
    "test": "vitest",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.19",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "^4.1.17",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^27.2.0",
    "oxlint": "^1.30.0",
    "oxlint-tsgolint": "^0.8.2",
    "prettier": "^3.7.3",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.14",
    "vue-tsc": "^3.1.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": [
      "prettier --write",
      "oxlint"
    ],
    "**/*.{json,css,scss,md}": "prettier --write"
  }
}